{
	"name": "Naver",
	"desc": "Naver",
	"url": "https://nid.naver.com",
	"oauth2": {
		"authorize": {
			"url": "/oauth2.0/authorize",
			"query": {
				"response_type": "code",
				"client_id": "{client_id}",
				"redirect_uri": "{{callback}}",
				"state": "{{state}}",
				"scope": "{scope}"
			}
		},
		"access_token": {
			"url": "/oauth2.0/token",
			"query": {
				"response_type": "code",
				"client_id": "{client_id}",
				"redirect_uri": "{{callback}}",
				"state": "{{state}}",
				"scope": "{scope}"
			}
		},
		"parameters": {
			"client_id": "string"
		}
	},
	"href": {
		"keys": "https://developers.naver.com",
		"docs": "https://developers.naver.com/docs",
		"apps": "https://developers.naver.com",
		"provider": "https://naver.com/"
	}
}